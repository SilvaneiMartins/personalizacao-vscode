    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.wordWrap": "on", // Ativa a quebra de linha
    "editor.wordWrapColumn": 120, // Define a coluna para quebra de linha
    "editor.wrappingIndent": "same", // Mantém a mesma indentação após a quebra
    "workbench.tree.indent": 18, // Define a indentação da árvore de arquivos
